// Load template APIs
const createRegex = require("createRegex");
const getCookieValues = require('getCookieValues');
const makeInteger = require("makeInteger");
const setCookie = require("setCookie");
const log = require("logToConsole");

// Create reference to param table
const cookies = data.cookies && data.cookies.length ? data.cookies : [];
log('managing cookie', data);

// Go through all the rows in the table and set each as a response cookie
cookies.forEach((cookie) => {
    // if the flag data.ifNotSet is true, check if the cookie is already set
    log('check if cookie sets only if not set', cookie.name, cookie.ifNotSet);
    if (cookie.ifNotSet) {
        const cookieValue = getCookieValues(cookie.name)[0];
        log('cookieValue for cookie', cookie.name, cookieValue);
        if (cookieValue) {
            return;
        }
    }

    // get the name, value
    const name = cookie.name;
    const value = cookie.value + '';

    // get the domain, path, secure, sameSite, and expiration
    const options = {
        domain: cookie.domain,
        path: "/",
        secure: true,
        sameSite: cookie.sameSite,
    };

    // get the expiration
    let expiration = cookie.expiration || "session";

    // calculate the correct cookie max age if it's not set to expire at the session
    if (expiration !== "session") {
        // identify the number
        const removeChars = createRegex("[a-zA-Z]+", "i");
        const maxAgeNumber = makeInteger(expiration.replace(removeChars, ""));

        // identify the type
        const removeNumbersRegex = createRegex("[0-9]+", "i");
        const type = expiration.replace(removeNumbersRegex, "");

        // default for days
        let multiplication = 1;
        if (type === "d") {
            multiplication = 24 * 60 * 60;
        } else if (type === "m") {
            multiplication = 30 * 24 * 60 * 60;
        } else if (type === "y") {
            multiplication = 12 * 30 * 24 * 60 * 60;
        }

        // create correct max age
        options["Max-Age"] = maxAgeNumber * multiplication;
    } else {
        // set the expiration to the session
        options.expires = expiration;
    }

    // set the httpOnly flag
    if (cookie.httpOnly) {
        options.HttpOnly = cookie.httpOnly;
    }

    // Only set cookie if value is valid, or if value is empty and the relevant setting is checked
    if (value || (value === "" && cookie.override)) {
        setCookie(name, value, options, !cookie.encode);
    }
});

data.gtmOnSuccess();


